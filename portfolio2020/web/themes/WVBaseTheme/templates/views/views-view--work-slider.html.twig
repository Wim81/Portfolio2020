{% set work_items = rows.0['#rows'] %}

<div class="front-page-projects col-12 mb-10">
    <div class="container">
        <div class="row">
            <div class="col-md-8 col-lg-6 offset-md-2 offset-lg-3">
                <div class="h2-wrapper mb-8">
                    <h2 class="intro-section-title">Projects</h2>
                </div>
            </div>
        </div>
    </div>

    <div class="view-work-slider">
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-lg-6 offset-md-2 offset-lg-3 slider-image-wrapper slick-mobile-no-arrows">
                    {% for work_item in work_items %}
                        {{ work_item }}
                    {% endfor %}
                </div>
            </div>
        </div>
    </div>
</div>


